{
  "version": "0.2.0",
  "configurations": [
      {
          "name": "Debug Terraform Provider",
          "type": "go",
          "request": "launch",
          "mode": "debug",
          // this assumes your workspace is the root of the repo
          "program": "${workspaceFolder}",
          "env": {
            "LEOSTREAM_API_SSL_INSECURE" : "True",
            "https_proxy": "http://localhost:8080",
            "HTTPS_PROXY": "http://localhost:8080",
        },
          "args": [
              "-debug",
          ]
      }
  ]
}
